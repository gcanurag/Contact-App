<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Contact App</title>
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css"
      integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm"
      crossorigin="anonymous"
    />
    <!-- <script src="./script.js"></script> -->
    
  </head>

  <body>
    <%-include("components/header")%>
    <% contact.map((c,i)=>{ %>
        <div id=<%-c.id %> class="card" style="width: 18rem">
        <div class="card-body">
            <h5 class="card-title">Name :<%-c.name %></h5>
            <p class="card-text">Contact: <%-c.contact%></p>
            <p class="card-text">Email: <%-c.email%></p>
            <a href="/edit/<%-c.id%>" class="btn btn-primary">Edit</a>
            <a  class="btn btn-danger" href="/delete/<%-c.id%>" >Delete</a>
            
        </div>
    </div>


    <% }) %>
   

  </body>

 
</html>
